{
  "name": "hello-world",
  "version": "1.0.0",
  "description": "I'm an Earthling!",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode development --config ./webpack.config.js",
    "build:prod": "webpack --mode production --display-max-modules 0",
    "clean": "rimraf ./.build/*{.js,.map} && rimraf ./dist && rimraf ./public/js/*{.js,.map} && rimraf ./public/styles/*{.css,.map} && rimraf ./public/images/*",
    "dev": "BABEL_ENV=development NODE_ENV=development run-s webpack -w --debug --display-max-modules 0 --mode development nodemon ./src/Server/index.js --inspect=9229",
    "prod": "BABEL_ENV=production NODE_ENV=production node ./src/Server/index.js",
    "lint": "eslint --fix 'tests/**/*.js' 'src/**/*.js' '.jest/**/*.js'",
    "prebuild": "yarn clean && mkdir dist",
    "prestart": "yarn --frozen-lockfile",
    "start": "BABEL_ENV=production NODE_ENV=production run-s clean build:prod && node ./index.js",
    "start:todo": "webpack --mode=development && node ./index.js",
    "start:debug": "npm-run-all build dev:debug",
    "test:watch": "jest --watch --env=jsdom --verbose",
    "test": "BABEL_ENV=test yarn lint && jest --no-watchman --env=jsdom",
    "watch": "webpack --mode development --debug -w",
    "storybook": "BABEL_ENV=storybook start-storybook -p 6006",
    "build-storybook": "BABEL_ENV=storybook build-storybook",
    "devServer": "NODE_ENV=development webpack-dev-server --mode development --hot --inline --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jking633/hello-world.git"
  },
  "author": "Jesse Raygoza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jking633/hello-world/issues"
  },
  "homepage": "https://github.com/jking633/hello-world#readme",
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.0.8",
    "@storybook/addon-notes": "^5.0.8",
    "@storybook/addon-options": "^5.0.8",
    "@storybook/addon-viewport": "^5.0.8",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.3",
    "@storybook/addon-actions": "^5.0.8",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.0.8",
    "@storybook/addon-links": "^5.0.8",
    "@storybook/addon-notes": "^5.0.8",
    "@storybook/addon-options": "^5.0.8",
    "@storybook/addon-viewport": "^5.0.8",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "babel-loader": "^8.0.5",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "ignore-styles": "^5.0.1",
    "jest": "^24.3.1",
    "jest-puppeteer": "^4.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-dev-utils": "^8.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  }
}
