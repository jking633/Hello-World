{
  "name": "hello-world",
  "version": "1.0.0",
  "description": "I'm an Earthling!",
  "main": "index.js",
  "scripts": {
    "//": "MAKE SURE THAT THIS IS COMMONJS READY",
    "build-server": "webpack --config ./webpack/webpack.server.js",
    "build-storybook": "BABEL_ENV=storybook build-storybook",
    "build:prod": "yarn clean && webpack --config ./webpack/webpack.server.js && webpack -p --config ./webpack/webpack.client.prod.js --mode production",
    "ci-postbuild": "npm install && npm run build:prod",
    "clean": "rm -rf build && rimraf ./.build/*{.js,.map} && rimraf ./dist && rimraf ./public/*{.js,.map} && rimraf ./public/styles/*{.css,.map} && rimraf ./public/images/*",
    "dev:build-client": "webpack --config ./webpack/webpack.client.js --watch",
    "dev:build-server": "webpack --config ./webpack/webpack.server.js --watch",
    "dev": "npm run --scripts-prepend-node-path build-server && npm-run-all --parallel dev:*",
    "example:build:dev": "webpack --mode development --config ./webpack.config.js",
    "example:build:prod": "webpack --mode production --display-max-modules 0",
    "example:dev:debug": "BABEL_ENV=development NODE_ENV=development node --inspect-brk=9229 ./index.js && webpack --debug --display-max-modules 0 --mode development",
    "example:dev:server": "yarn clean && webpack-dev-server --mode development --hot --history-api-fallback --open --debug --progress --env --colors",
    "example:prod": "BABEL_ENV=production NODE_ENV=production node ./src/Server/index.js",
    "example:start:debug": "npm-run-all build:dev dev",
    "example:start:example": "BABEL_ENV=production NODE_ENV=development run-s clean build:dev && node index.js",
    "example:start:todo": "yarn build:dev && node ./index.js",
    "example:watch": "webpack --mode development --debug -w",
    "lint": "eslint --fix 'tests/**/*.js' 'src/**/*.js' '.jest/**/*.js'",
    "prebuild": "yarn clean && mkdir dist",
    "precommit": "pretty-quick --staged",
    "prestart": "yarn --frozen-lockfile",
    "start:server": "nodemon ./build/server-bundle.js",
    "start:server:debug": "nodemon --inspect-brk ./build/server-bundle.js",
    "storybook": "BABEL_ENV=storybook start-storybook -p 6006",
    "test:watch": "jest --watch --env=jsdom --verbose",
    "test": "BABEL_ENV=test yarn lint && jest --no-watchman --env=jsdom",
    "webpack-prod": "webpack -p --progress --config ./webpack/webpack.client.prod.js --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jking633/hello-world.git"
  },
  "author": "Jesse Raygoza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jking633/hello-world/issues"
  },
  "homepage": "https://github.com/jking633/hello-world#readme",
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/runtime": "^7.4.3",
    "axios": "0.19.2",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^3.1.0",
    "concurrently": "5.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "intl": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "5.2.1",
    "react-hot-loader": "^4.8.3",
    "react-lazy-load-image-component": "^1.4.1",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serve-favicon": "^2.5.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-polyfill": "^1.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "assets-webpack-plugin": "^3.9.10",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "ignore-styles": "^5.0.1",
    "jest": "^24.3.1",
    "jest-puppeteer": "^4.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-dev-utils": "^8.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
